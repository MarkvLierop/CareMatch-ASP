
@{
    ViewBag.Title = "Hulpvraag";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!-- knopje voor verijderen en aanpassingen moeten mogelijk zijn-->
<h2>Hulpvraag</h2>
<div class="panel panel-info">
    <div class="panel-heading">
        <h3 class="panel-title">Hulpvraag: @ViewBag.Hulpvraag.Titel</h3>
    </div>
    <div class="panel-body">
        <table class="table table-striped">
            <thead>
            </thead>
            <tbody>
                <tr>
                    <th>Omschrijving</th>
                    <td>@ViewBag.Hulpvraag.HulpvraagInhoud</td>
                </tr>
                <tr>
                    <th>Urgent</th>
                    <td>@ViewBag.Hulpvraag.Urgent</td>
                </tr>
                <tr>
                    <th>Locatie</th>
                    <td>@ViewBag.Hulpvraag.Locatie</td>
                </tr>
                <tr>
                    <th>Startdatum</th>
                    <td>@ViewBag.Hulpvraag.StartDatum</td>
                </tr>
                <tr>
                    <th>Einddatum</th>
                    <td>@ViewBag.Hulpvraag.EindDatum</td>
                </tr>
                <tr>
                    <th>Frequentie</th>
                    <td>@ViewBag.Hulpvraag.Frequentie</td>
                </tr>
            </tbody>
        </table>
    </div>
</div>